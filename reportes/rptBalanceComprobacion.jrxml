	<queryString>
		<![CDATA[select
	order_print,
	empresa,
	tag,
	sum(monto_debe) debe,
	sum(monto_haber) haber
  from conta.balance_comprobacion
  where empresa = $P{nEmpresa} and intervalo between $P{periodo_inicio} and $P{periodo_final}
  group by
  	order_print,
	empresa,
	tag
  order by order_print]]>
